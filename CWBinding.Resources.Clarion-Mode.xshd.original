                <Key word="TODO"/>
                <Key word="FIXME"/>
            </KeyWords>
            <KeyWords name="WarningWords" bold="true" italic="false" color="#EEE0E000">
                <Key word="HACK"/>
                <Key word="UNDONE"/>
            </KeyWords>
        </RuleSet>
        <RuleSet name="XmlDocSet" ignorecase="true" noescapesequences="true">
            <Delimiters>&amp;&lt;&gt;~!@%^*()-+=|\#/{}[]:;"' ,	.?</Delimiters>
            <Span name="String" bold="true" italic="false" color="Silver" stopateol="true" noescapesequences="true">
