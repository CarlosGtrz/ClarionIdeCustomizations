            <Delimiters>&amp;&lt;&gt;~!@%^*()-+=|\#/{}[]:;"' ,	.?</Delimiters>
            <Span name="XmlTag" rule="XmlDocSet" bold="false" italic="false" color="Gray" stopateol="true" noescapesequences="true">
